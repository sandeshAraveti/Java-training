package com.hcl.day3;

public class Factorial
{
	
	public void fact(int n)
	{
		int fact=1;
		
		for( int i=1,i<=n,i++)
		{
			fact=fact*i;
		}
		
			System.out.println("Factorial of "+n" is :"+fact);
		}
		
	public static void main(String[] args)
			 {
		int n=6;
		Factorial obj=new Factorial();
		obj.fact(n);
	}
	}


