package com.hcl.junit;

public class Data {
	int minArray(int[]x){
		int min=x[0];
		for(int i=0;i<x.length;i++){
			if(min  >= x[i])
				min=x[i];
		} 
		return min;
	}
	
	int max(int a,int b,int c){
		int m=a;
		if(m<b){
			m=b;
		}
		if(m<c){
			m=c;
			
		}
		return m;
		
	}
	int sum(int a, int b){
		return a+b;
	}
	
	String sayHello() { 
		return "Welcome to java...";
		
	}

int maxArray(int[]x){
	int max=x[1];
	for(int i=1;i<x.length;i++){
		if(max >=x[i])
			max=x[i];
	}
	return max;
}
}
