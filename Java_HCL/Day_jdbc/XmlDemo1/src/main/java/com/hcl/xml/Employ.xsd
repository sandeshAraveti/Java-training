<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="EMPS" type="complexTypeEmployData" />
<xsd:complexType name="complexTypeEmployData">
	<xsd:sequence>
		<xsd:element name="EMP" type="complexTypeEmploy" maxOccurs="unbounded" />
	</xsd:sequence>
</xsd:complexType>	
<xsd:complexType name="complexTypeEmploy">
	<xsd:sequence>
		<xsd:element name="EMPNO" type="xsd:positiveInteger"/>
		<xsd:element name="NAME" type="xsd:string"/>
		<xsd:element name="BASIC" type="xsd:positiveInteger"/>
			</xsd:sequence>
	
	<xsd:attribute name="CATEGORY" type="xsd:string" />
</xsd:complexType>
</xsd:schema>
